{
  "name": "vr-story",
  "version": "0.0.1",
  "description": "Story in VR",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --config ./webpack.dev.config.js --watch-poll",
    "build": "webpack --progress --colors --config ./webpack.dev.config.js",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r app gh-pages && cp index.html build/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "gh-pages": "npm run ghpages"
  },
  "author": "Sasha Kastsiushkin",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngokevin/aframe-react-boilerplate.git"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "ghpages": "0.0.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "aframe": "^0.2.0",
    "jquery": "^3.0.0"
  }
}
